{
    "name": "spacebib/peformance-stats",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Spacebib\\PeformanceStats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spacebib\\PeformanceStats\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "youxingxiang",
            "email": "xingxiang@spacebib.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "~5.5|~6.0|~7.0|~8.0",
        "datadog/php-datadogstatsd": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spacebib\\PeformanceStats\\PeformanceStatsServiceProvider"
            ]
        }
    }
}
